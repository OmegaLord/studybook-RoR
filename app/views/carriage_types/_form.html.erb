<%= form_with(model: carriage_type, local: true, class: 'form-horizontal') do |form| %>
  <%= render 'common/error', resource: @carriage_type %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
